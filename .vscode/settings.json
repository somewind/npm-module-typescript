{
  "javascript.validate.enable": false,
  "workbench.iconTheme": "vscode-icons",
  "eslint.options": {
    "extensions": [".ts", ".tsx", ".js"]
  },
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
